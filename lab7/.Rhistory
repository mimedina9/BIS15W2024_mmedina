library("tidyverse")
library("janitor")
library("skimr")
library("palmerpenguins")
glimpse(penguins)
levels(penguins$island)
penguins %>%
group_by(island) %>%
summarize(mean_body_mass_g=mean(body_mass_g ))
penguins %>%
group_by(island) %>%
summarize(mean_body_mass_g=mean(body_mass_g ),
n=n())
penguins %>%
group_by(island) %>%
summarize(number_NAs=sum(is.na(body_mass_g )))
penguins %>%
filter(!is.na(body_mass_g )) %>% #pull out all of the observations with a number
group_by(island) %>%
summaize(mean_body_mass_g=mean(body_mass_g),
n=n())
penguins %>%
filter(!is.na(body_mass_g ))%>% #pull out all of the observations with a number
group_by(island) %>%
summarize(mean_body_mass_g=mean(body_mass_g),
n=n())
penguins %>%
group_by(species, island) %>%
summarize(n=n(), .groups= 'keep')#the .groups argument here just prevents a warning message
penguins %>%
count(island, sort = T) #sort=T sorts the column in descending order
penguins %>%
group_by(island) %>%
summarize(n=n())
penguins %>%
count(island, species, sort = T) # sort=T will arrange in descending order
penguins %>%
tabyl(island, species)
penguins %>%
count(island, species, bill_length_mm)
penguins %>%
tabyl(species, bill_length_mm)
penguins %>%
group_by(species) %>%
summarize(mean_bill_length_mm=mean(bill_length_mm, na.rm=T))
penguins %>%
summarize(distinct_species = n_distinct(species),
distinct_island = n_distinct(island),
distinct_sex = n_distinct(sex))
penguins %>%
summarize(across(c(species, island, sex), n_distinct))
penguins %>%
summarize(across(contains("mm"), mean, na.rm=T))
penguins %>%
summarize(across(contains("mm"), \(x) mean(x, na.rm = TRUE)))
penguins %>%
group_by(sex) %>%
summarize(across(contains("mm"), mean, na.rm=T))
penguins %>%
summarise_all(mean, na.rm=T)
penguins %>%
summarise(across(!c(species, island, sex, year),
mean, na.rm=T))
penguins %>%
summarise(across(starts_with("bill"), mean, na.rm=T))
